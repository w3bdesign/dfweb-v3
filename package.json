{
  "name": "dfweb-v3",
  "version": "1.4.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": " jest --coverage --collectCoverageFrom='src/components/**/*.{js,jsx,ts,tsx}'",
    "test:watch": "jest --watch",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run",
    "chromatic": "chromatic --exit-zero-on-changes",
    "e2e": "start-test dev 3000 cypress:headless",
    "playwright": "npx playwright test",
    "playwright:codegen": "playwright codegen",
    "refresh": "rm -rf node_modules && rm pnpm-lock.yaml && pnpm store prune && pnpm i && pnpm format",
    "prettier": "prettier \"**/*.{js,mjs,ts,tsx,md,mdx,json,yml,css}\" --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "prettier:fix": "npm run prettier -- --write"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@sanity/image-url": "^1.0.2",
    "canvas": "^2.11.2",
    "framer-motion": "^11.2.10",
    "next": "14.2.3",
    "next-sanity": "^9.3.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",    
    "react-portable-text": "^0.6.0",
    "react-use": "^17.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@babel/core": "^7.24.7",
    "@playwright/test": "^1.44.1",
    "@storybook/addon-a11y": "7.6.19",
    "@storybook/addon-actions": "7.6.19",
    "@storybook/addon-essentials": "7.6.19",
    "@storybook/addon-links": "7.6.19",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/nextjs": "7.6.19",
    "@storybook/react": "7.6.19",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.19",
    "axe-core": "^4.9.1",
    "babel-loader": "^9.1.3",
    "chromatic": "^11.5.3",
    "cypress": "13.11.0",
    "cypress-axe": "^1.5.0",
    "eslint": "9.4.0",
    "eslint-config-cypress": "^0.28.0",
    "eslint-config-next": "^14.2.3",
    "eslint-plugin-jest-dom": "^5.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.0",
    "jsdom-testing-mocks": "^1.13.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "react-test-renderer": "18.3.1",
    "sitemap": "^8.0.0",
    "start-server-and-test": "^2.0.4",
    "storybook": "7.6.19",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "pnpm": {
    "packageExtensions": {
      "posix": {
        "optionalDependencies": {
          "node-gyp": "*"
        }
      }
    }
  }
}
